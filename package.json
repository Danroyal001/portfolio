{
  "name": "portfolio",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "data": "ts-node --skip-project data.ts",
    "data:cli": "ts-node --skip-project data.ts -S",
    "postinstall": "npm --prefix functions install",
    "deploy": "firebase deploy",
    "deploy:cli": "firebase deploy --non-interactive --token $FIREBASE_TOKEN",
    "dev:ssr": "ng run portfolio:serve-ssr",
    "serve:ssr": "node dist/portfolio/server/main.js",
    "build:ssr": "ng build --prod && ng run portfolio:server:production",
    "prerender": "ng run portfolio:prerender",
    "screenshots": "start-server-and-test 'ng serve --port=5000' 5000 'ts-node --skip-project screenshots.ts'"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "~9.1.7",
    "@angular/cdk": "~9.2.3",
    "@angular/common": "~9.1.7",
