{
  "name": "portfolio",
  "version": "0.0.0",
  "description": "A portfolio designed for lazy developers all over the world. With little configuration and maintanince let this application automatically update itself without you having to lift a finger!",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "data": "ts-node --skip-project scripts/index.ts",
    "data:cli": "ts-node --skip-project scripts/index.ts -S",
    "postinstall": "npm --prefix functions install",
    "deploy": "firebase deploy",
    "deploy:cli": "firebase deploy --non-interactive --token $FIREBASE_TOKEN",
    "dev:ssr": "ng run portfolio:serve-ssr",
    "serve:ssr": "node dist/portfolio/server/main.js",
    "build:ssr": "ng build --prod && ng run portfolio:server:production",
    "prerender": "ng run portfolio:prerender"
  },
  "license": "MIT",
  "private": false,
  "dependencies": {
    "@angular/animations": "~10.0.1",
    "@angular/cdk": "~10.0.0",
    "@angular/common": "~10.0.1",
    "@angular/compiler": "~10.0.1",
    "@angular/core": "~10.0.1",
    "@angular/fire": "^6.0.2",
    "@angular/forms": "~10.0.1",
    "@angular/material": "~10.0.0",
    "@angular/platform-browser": "~10.0.1",
    "@angular/platform-browser-dynamic": "~10.0.1",
    "@angular/platform-server": "~10.0.1",
    "@angular/router": "~10.0.1",
    "@angular/service-worker": "~10.0.1",
    "@nguniversal/express-engine": "^10.0.0",
    "bootstrap": "^4.5.0",
    "express": "^4.17.1",
    "firebase": "^7.15.5",
    "hammerjs": "^2.0.8",
    "ngmeta": "^9.2.0",
    "rxjs": "~6.5.5",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.0",
    "@angular/cli": "~10.0.0",
    "@angular/compiler-cli": "~10.0.1",
    "@nguniversal/builders": "^10.0.0",
    "@types/express": "^4.17.6",
    "@types/jasmine": "~3.5.11",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^12.12.6",
    "@types/puppeteer": "^2.1.0",
    "codelyzer": "^5.1.2",
    "dotenv": "^8.2.0",
    "firebase-admin": "^8.13.0",
    "firebase-tools": "^8.4.3",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "jsdom": "^16.2.2",
    "karma": "~5.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "node-fetch": "^2.6.0",
    "png-to-ico": "^2.1.0",
    "prompts": "^2.3.2",
    "protractor": "~7.0.0",
    "puppeteer": "^3.0.4",
    "rimraf": "^3.0.2",
    "scroll-to-bottomjs": "^1.1.0",
    "sharp": "^0.25.4",
    "sitemap": "^6.1.6",
    "ts-node": "~8.10.2",
    "tslint": "~6.1.2",
    "typescript": "~3.9.5",
    "webp-converter": "^2.2.3"
  }
}
